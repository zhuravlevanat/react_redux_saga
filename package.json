    "redux": "4.0.1",
    "redux-saga": "^1.1.3",
    "sinon": "7.2.3",
    "styled-components": "4.1.3",
    "styled-components-test-utils": "1.0.1",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.2",
    "uuidv4": "^6.0.2",
    "webpack-node-externals": "1.7.2"
  }
}
